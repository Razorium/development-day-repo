datasource db {
  provider = "postgresql" // or your preferred database
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id          Int     @id @default(autoincrement())
  familyName  String
  givenName   String
  email       String  @unique
  age         Int
  country     String
  phoneNumber String
  occupation  String
  university  String?
}
